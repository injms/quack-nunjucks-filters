{
  "name": "@injms/quack-nunjucks-filters",
  "version": "0.12.0",
  "description": "A collections of filters to use with Nunjucks and Eleventy.",
  "license": "SEE LICENSE IN license.md",
  "author": {
    "name": "Ian James",
    "url": "https://inj.ms"
  },
  "keywords": [
    "nunjucks",
    "filters",
    "eleventy",
    "11ty"
  ],
  "homepage": "https://github.com/injms/quack-nunjucks-filters#readme",
  "bugs": {
    "url": "https://github.com/injms/quack-nunjucks-filters/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/injms/quack-nunjucks-filters.git"
  },
  "scripts": {
    "lint": "npm run lint:javascript && npm run lint:tests",
    "lint:javascript": "eslint --config='.eslintrc.yaml' 'filters/**/*.js'",
    "lint:tests": "eslint --config='.eslintrc.yaml' '__spec__/**/*.js'",
    "test": "mocha '__spec__/**/*.spec.js'",
    "create-types": "tsc --declaration --allowJs --emitDeclarationOnly index.js"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "filters/",
    "license.md",
    "readme.md"
  ],
  "directories": {
    "test": "__spec__"
  },
  "engines": {
    "node": ">= 14.13.0",
    "npm": ">= 6.14.8"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.5",
    "clean-css": "^5.0.1",
    "markdown-it": "^12.0.2",
    "nunjucks": "^3.2.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^8.2.1",
    "sinon": "^9.2.1"
  }
}
